<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:localsPage="clr-namespace:MixNativeBlazor"
       xmlns:globalPages="clr-namespace:MixNativeBlazor.Pages"
       Shell.FlyoutBehavior="Disabled"
       x:Class="MixNativeBlazor.AppShell">
     <ShellContent x:Name="SplashScreen"
                   ContentTemplate="{DataTemplate localsPage:MainPage}"
                   Route="MainPage"/>
     <ShellContent ContentTemplate="{DataTemplate globalPages:PerfilPage}"
                   Route="PerfilPage"/>
</Shell>
